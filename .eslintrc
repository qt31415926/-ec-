{
  extends: 'airbnb-base',
  ecmaFeatures: {
    "modules": true
  },
  env: {
    "node": true
  },
  plugins: ['babel', 'import'],
  parser: 'babel-eslint',
  rules: {
    "array-bracket-spacing": 2,
    "arrow-body-style": 2,
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": [
      "error",
      "never"
    ],
    "func-names": 0, 
    "keyword-spacing": 2,
    "max-len": 0,
    "new-cap": 0,
    "no-array-constructor": 2,
    "no-case-declarations": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-debugger": 0,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-param-reassign": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "object-curly-spacing": 0,
    "one-var": 2,
    "padded-blocks": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-rest-params": 2,
    "prefer-template": 1,
    "prefer-spread": 2,
    "quote-props": 2,
    "semi": "never",
    "space-before-blocks": 2,
    "space-before-function-paren": 0,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "no-restricted-syntax": 0,
    "global-require": 0
  },
}
